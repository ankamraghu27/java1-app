pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
git branch: 'main', changelog: false, credentialsId: '6d364932-1dcf-4658-8846-aeae8526ec91', poll: false, url: 'https://github.com/ankamraghu27/java1-app.git'
 sh "mvn -version"
 sh "mvn clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
sh "mvn test"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
sh "mvn compile"
}
}
stage('package') 
{
steps{
echo "Compiling the Project.........."
sh "mvn package"
}
}
}
}
